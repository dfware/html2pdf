{
    "name": "dfware/html2pdf",
    "description": "Provides support for HTML to PDF and PHP to PDF conversion",
    "keywords": ["yii2", "html2pdf", "php2pdf", "pdf", "convert", "conversion", "template"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/dfware/html2pdf/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "https://github.com/dfware/html2pdf/wiki",
        "source": "https://github.com/dfware/html2pdf"
    },
    "authors": [
        {
            "name": "Pavel Karelin",
            "email": "devpastet@yandex.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.14"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.27|^5.0|^6.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": { "yii2tech\\html2pdf\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
